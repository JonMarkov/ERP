<view class='out'>
  <!-- 基本信息 -->
  <view class='out_basic'>
    <view class='basic_content'>
      <text>申请人员：</text>
      <text>{{user.user_name}}</text>
    </view>
    <view class='basic_content'>
      <text>申请日期：</text>
       <view class='sele'>
        <picker mode="date" value="{{date}}" start="" end="" bindchange="bindSQRQDATEChange">
          {{SQRQdate}}
        </picker>
        <picker mode="time" value="{{time}}" start="" end="" bindchange="bindSQRQTIMEChange">
          {{SQRQtime}}
        </picker>
      </view>
    </view>
    <view class='basic_content'>
      <text>项目名称：</text>
      <input maxlength="10" bindinput='ToXMMC' placeholder="请输入项目名称" placeholder-class="placeholder_input" />
    </view>
    <view class='basic_content'>
      <text>合同金额:</text>
      <input maxlength="10" bindinput='ToHTJE' placeholder="请输入合同金额" placeholder-class="placeholder_input" />
    </view>
    <view class='basic_content'>
      <text>签约甲方:</text>
      <input maxlength="10" bindinput='ToQYJF' placeholder="请输入签约甲方" placeholder-class="placeholder_input" />
    </view>
    <view class='basic_content'>
      <text>甲方负责人:</text>
      <input maxlength="10" bindinput='ToJFFZR' placeholder="请输入负责人姓名" placeholder-class="placeholder_input" />
    </view>
     <view class='basic_content'>
      <text>乙方负责人:</text>
      <input maxlength="10" bindinput='ToYFFZR' placeholder="请输入负责人姓名" placeholder-class="placeholder_input" />
    </view>
    <view class='basic_content'>
      <text>签约乙方:</text>
      <input maxlength="10" bindinput='ToQYYF' placeholder="请输入签约乙方" placeholder-class="placeholder_input" />
    </view>
    <view class='basic_content'>
      <text>合同号:</text>
      <input maxlength="10" bindinput='ToHTH' placeholder="请输入合同号" placeholder-class="placeholder_input" />
    </view>
    <view>
      <button bindtap='TJht'>提交</button>
    </view>
  </view>
</view>